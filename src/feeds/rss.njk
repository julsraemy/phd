<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
<channel>
    <title>LOUD for Cultural Heritage – PhD Thesis by Julien A. Raemy</title>
    <link>{{ site.url }}</link>
    <description>A website about my PhD – Linked Open Usable Data for Cultural Heritage: Perspectives on Community Practices and Semantic Interoperability</description>
    <language>en-gb</language>
    {% for item in collections.sitePages %}
    <item>
        <title>{{ page.data.title }}</title>
        <link>{{ site.url }}{{ item.url | url }}</link>
        <guid isPermaLink="true">{{ site.url }}{{ item.url | url }}</guid>
        <pubDate>{{ item.date | dateToRfc3339 }}</pubDate>
        <description><![CDATA[{{ item.templateContent | safe }}]]></description>
    </item>
    {% endfor %}
</channel>
</rss>
